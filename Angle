
public class Main {
   public double x;
   public double y;

   public Main(double x, double y) {
      this.x = x;
      this.y = y;
  }

   public double angle() {

      double angleInRadians = Math.atan(y / x);

      double angleInDegrees = Math.toDegrees(angleInRadians);
      return angleInDegrees;
      
  }
  @Override
  public String toString() {
   return "(" + x + ", " + y + ")";
}
public static void main(String[] args) {
   double x = 3;
   double y = 4;

   Main point = new Main(x, y);

   double angle = point.angle();
   System.out.println("The angle is: " + angle + " degrees");
}
}
